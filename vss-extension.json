{
  "manifestVersion": 1,
  "id": "extension_id",
  "name": "GoReleaser",
  "version": "extension_version",
  "publisher": "extension_publisher",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "GoReleaser integration for Azure DevOps",
  "categories": [
    "Azure Pipelines"
  ],
  "public": extension_public,
  "icons": {
    "default": "images/extension-icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "home": {
      "uri": "https://goreleaser.com/"
    },
    "getstarted": {
      "uri": "https://goreleaser.com/quick-start/"
    },
    "support": {
      "uri": "https://github.com/goreleaser/goreleaser/discussions"
    },
    "license": {
      "uri": "./LICENSE"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/goreleaser/goreleaser-azure-devops-extension"
  },
  "files": [
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "goreleaserTask"
    },
    {
      "path": "goreleaserInstallerTask"
    },
    {
      "path": "LICENSE",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "goreleaserTask"
      }
    },
    {
      "id": "goreleaser-installer-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "goreleaserInstallerTask"
      }
    }
  ]
}

